# DB_PASSWORD=postgres
# DB_DATABASE=phd_admin

# Backend Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_PREFIX=api
CORS_ORIGIN=*

# Logging
LOG_LEVEL=debug
